---
import Layout from "@src/layouts/Doc.astro"
import { ContentCollectionStaticPaths } from "@src/utils/routing/static-paths"

export async function getStaticPaths() {
  const routing = new ContentCollectionStaticPaths<"dapps">("dapps")
  const paths = await routing.paths()

  return paths
}

const { Content } = await Astro.props.entry.render()

const paths = await getStaticPaths()
---

<Layout paths={paths} {...Astro.props}>
  <Content />
</Layout>
